<h1>List of All Songs</h1>

<tbody>
<% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
    <td><%= song.release_year %></td>
    <td><%= song.released %></td>

    <td>
    <%= link_to "Song", song %>
    <%= link_to "Create Song", new_song_path %>
    <%= link_to "Edit Song", edit_song_path(song) %>
    <%= link_to "Delete Song", song, method: :delete, data: { confirm: 'Foreal?'} %>
    </td>
  </tr>
<% end %>
</tbody>
